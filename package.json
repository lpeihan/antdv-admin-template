    "serve": "cross-env NODE_ENV=development ENV=local nodemon --exec tsx build/dev-server.ts",
    "start": "npm run serve",
    "build": "cross-env NODE_ENV=production tsx build/build.ts",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true npm run build",
